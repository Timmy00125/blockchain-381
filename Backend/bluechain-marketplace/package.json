{
  "name": "bluechain-marketplace",
  "version": "1.0.0",
  "description": "A blockchain-powered e-commerce system",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "elliptic": "^6.5.4",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "pg": "^8.11.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}